{
  "name": "@codeforamerica/safety-net-blueprint-contracts",
  "version": "1.0.0",
  "description": "Behavioral contracts, OpenAPI specs, validation, and overlay resolution for Safety Net APIs",
  "type": "module",
  "files": [
    "src/",
    "*.yaml",
    "components/",
    "patterns/",
    "overlays/",
    "authored/",
    "scripts/"
  ],
  "bin": {
    "safety-net-resolve": "./scripts/resolve.js",
    "safety-net-design-reference": "./scripts/export-design-reference.js"
  },
  "exports": {
    "./overlay": "./src/overlay/overlay-resolver.js",
    "./validation": "./src/validation/openapi-validator.js",
    "./loader": "./src/validation/openapi-loader.js",
    "./patterns": "./src/validation/pattern-validator.js"
  },
  "scripts": {
    "prepack": "npm run design:reference && node ../../scripts/build-package-readme.js",
    "test": "node --test tests/unit/",
    "validate": "npm run validate:syntax && npm run validate:patterns && npm run validate:schemas",
    "validate:schemas": "node scripts/validate-schemas.js --spec=.",
    "validate:syntax": "node scripts/validate-openapi.js --spec=.",
    "validate:lint": "npx spectral lint '*-openapi.yaml' --ignore-unknown-format",
    "validate:patterns": "node scripts/validate-patterns.js --spec=.",
    "resolve": "node scripts/resolve.js --spec=. --out=../resolved",
    "api:new": "node scripts/generate-api.js",
    "api:update": "node scripts/update-api.js",
    "design:reference": "node scripts/export-design-reference.js --spec=. --out=../../docs",
    "contract-tables:export": "node scripts/export-contract-tables.js",
    "contract-tables:import": "node scripts/import-contract-tables.js"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "js-yaml": "^4.1.0"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeforamerica/safety-net-blueprint",
    "directory": "packages/contracts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@stoplight/spectral-cli": "^6.15.0"
  }
}
